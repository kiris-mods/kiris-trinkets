{
	"schema_version": 1,
	"quilt_loader": {
		"id": "kirismoddingtemplate",
		"group": "${group}",
		"version": "${version}",
		"metadata": {
			"name": "Mysterious Biomes",
			"description": "Kiri's personal template for modding on Quilt.",
			"contributors": {
				"KiriCattus": "Owner/Author"
			},
			"authors": [
				{
					"name": "KiriCattus",
					"contact": {
						"github": "KiriCattus",
						"homepage": "https://tophatcat.dev"
					}
				}
			],
			"contact": {
				"homepage": "https://modrinth.com/mod/kiris-modding-template",
				"issues": "https://github.com/tophatcats-mods/kiris-modding-template/issues",
				"sources": "https://github.com/tophatcats-mods/kiris-modding-template"
			},
			"license": {
				"name": "Lesser General Public License v2.1",
				"id": "LGPL-2.1",
				"url": "https://github.com/tophatcats-mods/kiris-modding-template/blob/dev/LICENSE.md"
			},
			"icon": "assets/kirismoddingtemplate/logo.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
