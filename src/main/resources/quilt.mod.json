			},
			{
				"id": "minecraft",
				"versions": "=1.20.1"
			},
			{
				"id": "midnightlib",
				"version": ">=1.4.1"
			}
		]
	},
	"mc-publish": {
		"loaders": [
			"quilt"
		],
		"curseforge": 100000,
		"modrinth": "SSSSSSSS"
	},
	"modmenu": {
		"links": {
			"modmenu.discord": "https://discord.tophatcat.dev",
			"modmenu.modrinth": "https://modrinth.com/mod/kiris-modding-template",
			"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/kiris-modding-template"
		},
		"update_checker": false
	}
